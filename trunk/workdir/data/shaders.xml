<shaders path = "data/shaders/">
	<!--VERTEX SHADERS-->
	<vertex_shaders>
		<!--forward-->
		<shader name="AnimatedModelForward" file="Forward/AnimatedModel.fx" entry_point="mainVertexShader" vertex_type="PositionWeightIndicesNormalUV" preprocessor=""/>
		<shader name="PositionNormalForward" file="Forward/PositionNormal.fx" entry_point="VS" vertex_type="PositionNormal" preprocessor=""/>
		<shader name="PositionNormalUVForward" file="Forward/PositionNormalUV.fx" entry_point="VS" vertex_type="PositionNormalUV" preprocessor=""/>
		<shader name="PositionNormalUVUV2Forward" file="Forward/PositionNormalUVUV2.fx" entry_point="VS" vertex_type="PositionNormalUVUV2" preprocessor=""/>
		<shader name="PositionBumpUVForward" file="Forward/PositionBumpUV.fx" entry_point="VS" vertex_type="PositionBumpUV" preprocessor=""/>
		
		<!--
		

		<shader name="AnimatedModelForward" file="Forward/UberShader.fx" entry_point="VS" vertex_type="PositionWeightIndicesNormalUV" preprocessor="WEIGHTIDX; UV"/>
		<shader name="PositionNormalForward" file="Forward/UberShader.fx" entry_point="VS" vertex_type="PositionNormal" preprocessor=""/>
		<shader name="PositionNormalUVForward" file="Forward/UberShader.fx" entry_point="VS" vertex_type="PositionNormalUV" preprocessor="UV"/>
		<shader name="PositionNormalUVUV2Forward" file="Forward/UberShader.fx" entry_point="VS" vertex_type="PositionNormalUVUV2" preprocessor="UV; UV2"/>
		<shader name="PositionBumpUVForward" file="Forward/UberShader.fx" entry_point="VS" vertex_type="PositionBumpUV" preprocessor="BUMP; UV"/>
		
		-->
		<!--deferred-->
		<shader name="PositionNormalDeferred" file="Deferred/UberShader.fx" entry_point="VS" vertex_type="PositionNormal" preprocessor=""/>
		<shader name="PositionNormalUVDeferred" file="Deferred/UberShader.fx" entry_point="VS" vertex_type="PositionNormalUV" preprocessor="USE_UV"/>
		<shader name="PositionNormalUVUV2Deferred" file="Deferred/UberShader.fx" entry_point="VS" vertex_type="PositionNormalUVUV2" preprocessor="USE_UV;USE_UV2"/>
		<shader name="PositionBumpUVDeferred" file="Deferred/UberShader.fx" entry_point="VS" vertex_type="PositionBumpUV" preprocessor="USE_BUMP;USE_UV"/>
		
		<!--post process-->
		<shader name="NoiseVignetting" file="PostProcess/NoiseVignetting.fx" entry_point="VS" vertex_type="PositionUV" preprocessor=""/>
		<!--
		<shader name="Fog" file="PostProcess/Fog.fx" entry_point="VS" vertex_type="PositionUV" preprocessor=""/>
		-->
		
		<!--other-->
		
		<shader name="Skybox" file="Skybox.fx" entry_point="VS" vertex_type="PositionNormalUV" preprocessor=""/>
		<shader name="Quad" file="Quad.fx" entry_point="VS" vertex_type="PositionUV" preprocessor=""/>
		<shader name="DeferredQuad" file="Deferred/DeferredShading.fx" entry_point="VS" vertex_type="PositionNormal" preprocessor=""/>
		<!--
		
		-->
	</vertex_shaders>     
	<!--PIXEL SHADERS-->  
	<pixel_shaders>
		<!--forward-->
		
		
		<shader name="AnimatedModelForward" file="Forward/AnimatedModel.fx" entry_point="PS" preprocessor=""/>		
		<shader name="PositionNormalForward" file="Forward/PositionNormal.fx" entry_point="PS" preprocessor=""/>
		<shader name="PositionNormalUVForward" file="Forward/PositionNormalUV.fx" entry_point="PS" preprocessor=""/>		
		<shader name="PositionNormalUVUV2Forward" file="Forward/PositionNormalUVUV2.fx" entry_point="PS" preprocessor=""/>
		<shader name="PositionBumpUVForward" file="Forward/PositionBumpUV.fx" entry_point="PS" preprocessor=""/>
		
		<!--deferred-->
		<shader name="PositionNormalDeferred" file="Deferred/UberShader.fx" entry_point="PS" preprocessor=""/>
		<shader name="PositionNormalUVDeferred" file="Deferred/UberShader.fx" entry_point="PS" preprocessor="USE_UV"/>
		<shader name="PositionNormalUVUV2Deferred" file="Deferred/UberShader.fx" entry_point="PS" preprocessor="USE_UV;USE_UV2"/>
		<shader name="PositionBumpUVDeferred" file="Deferred/UberShader.fx" entry_point="PS" preprocessor="USE_BUMP;USE_UV"/>

		
		<!--post process-->
		<shader name="NoiseVignetting" file="PostProcess/NoiseVignetting.fx" entry_point="PS" preprocessor=""/>	
		<!--
		<shader name="Fog" file="PostProcess/Fog.fx" entry_point="PS" preprocessor=""/>	
		<shader name="ZBlur" file="PostProcess/ZBlur.fx" entry_point="PSZBlur" preprocessor=""/>	
		<shader name="Blur" file="PostProcess/Blur.fx" entry_point="ZBlurPS" preprocessor=""/>
		-->	
		
		<!--other-->
		<shader name="Skybox" file="Skybox.fx" entry_point="PS"  preprocessor=""/>
		<shader name="Quad" file="Quad.fx" entry_point="PS" preprocessor=""/>
		<shader name="DeferredQuad" file="Deferred/DeferredShading.fx" entry_point="PS" preprocessor=""/>
	</pixel_shaders>
</shaders> 